{
  "name": "ts-ice",
  "version": "0.1.1",
  "type": "module",
  "description": "üç® The easy way to make your clean REST client library.",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "build": "tsup",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky"
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "Ryan Martin",
  "license": "MIT",
  "devDependencies": {
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "3.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "defu": "^6.1.4"
  },
  "tsup": {
    "entry": [
      "./src/index.ts"
    ],
    "target": "es2021",
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true,
    "minify": true,
    "outDir": "dist",
    "clean": true,
    "treeshake": true
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
